{
	"@external": {
		"prefix": ["@external"],
		"body": ["/**", " * @external {${1:identifier}} ${2:URL}", " */"],
		"description": "syntax: `@external <identifier> <URL>`"
	},
	"@typedef (single)": {
		"prefix": ["@typedef"],
		"body": ["/** @typedef {${1:TYPE}} ${1:name} */"],
		"description": "syntax: `@typedef <type> <property>`"
	},
	"@typedef": {
		"prefix": ["@typedef"],
		"body": ["@typedef {${1:TYPE}} ${1:name}"],
		"description": "syntax: `@typedef <type> <property>`"
	}
}
